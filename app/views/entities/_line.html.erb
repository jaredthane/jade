<div class="movement">
  <% fields_for_movement(movement) do |movement_form| %>
		<div class="table_row">
			 <div class="table_cell_20"><%= movement_form.collection_select(:movement_type_id, get_movement_types, :id, :name) %></div>
		  <div class="table_cell_20"><%= movement_form.collection_select(:product_id, get_products, :id, :name) %></div>
			<div class="table_cell_20"><%= movement_form.text_field :quantity, :size => 5 %></div>
		  <div class="table_cell_20"><%= movement_form.collection_select(:user_id, get_users, :id, :name) %></div>
		  <div class="table_cell_20"><%= movement_form.text_field :date.to_date.to_s %></div>
		</div>
  <% end %>
</div>


