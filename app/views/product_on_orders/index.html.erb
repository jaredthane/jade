<h1>Listing product_on_orders</h1>

<table>
  <tr>
    <th>Product</th>
    <th>Order</th>
    <th>Quantity</th>
    <th>Price</th>
    <th>Received</th>
  </tr>

<% for product_on_order in @product_on_orders %>
  <tr>
    <td><%=h product_on_order.product %></td>
    <td><%=h product_on_order.order %></td>
    <td><%=h product_on_order.quantity %></td>
    <td><%=h product_on_order.price %></td>
    <td><%=h product_on_order.received %></td>
    <td><%= link_to 'Show', product_on_order %></td>
    <td><%= link_to 'Edit', edit_product_on_order_path(product_on_order) %></td>
    <td><%= link_to 'Destroy', product_on_order, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New product_on_order', new_product_on_order_path %>
